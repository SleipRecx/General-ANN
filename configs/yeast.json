{
  "dataset": {
    "name": "yeast",
    "case_fraction": 1.0,
    "validation_fraction": 0.1,
    "test_fraction": 0.1
  },
  "arcitechture": {
    "input_size": 8,
    "layer_specification": [40, 40, 40, 10],
    "activation_functions": ["sigmoid", "sigmoid", "sigmoid", "relu"],
    "weight_range": {
      "from": 0.0,
      "to": 0.2
    },
    "bias_range": {
      "from": 0.0,
      "to": 0.2
    }
  },
  "training": {
    "epochs": 1500,
    "learning_rate": 0.009,
    "minibatch_size": 20,
    "loss_function": "CE",
    "optimizer": "ADAM",
    "test_frequency": 10
  },
  "visualization": {
    "on": false,
    "map_batch_size": 10,
    "display_weights": [],
    "display_biases": [],
    "display_layers": [],
    "dendrogram_layers": []
  }
}