{
  "dataset": {
    "name": "mnist",
    "case_fraction": 0.4,
    "validation_fraction": 0.1,
    "test_fraction": 0.1
  },
  "arcitechture": {
    "input_size": 784,
    "layer_specification": [50, 10],
    "activation_functions": ["relu", "softmax"],
    "weight_range": {
      "from": -0.1,
      "to": 0.1
    },
    "bias_range": {
      "from": -0.1,
      "to": 0.1
    }
  },
  "training": {
    "epochs": 100,
    "learning_rate": 0.0001,
    "minibatch_size": 100,
    "loss_function": "CE",
    "optimizer": "ADAM",
    "test_frequency": 1
  },
  "visualization": {
    "on": false,
    "map_batch_size": 10,
    "display_weights": [],
    "display_biases": [],
    "display_layers": [],
    "dendrogram_layers": []
  }
}