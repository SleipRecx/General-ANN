  },
  "training": {
    "epochs": 1000,
    "learning_rate": 0.001,
    "minibatch_size": 1,
    "loss_function": "CE",
    "optimizer": "ADAM",
    "test_frequency": 100
  },
  "visualization": {
    "on": false,
    "map_batch_size": 10,
    "display_weights": [],
    "display_biases": [],
