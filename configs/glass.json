{
  "dataset": {
    "name": "glass",
    "case_fraction": 1.0,
    "validation_fraction": 0.1,
    "test_fraction": 0.1
  },
  "arcitechture": {
    "input_size": 9,
    "layer_specification": [50,15, 6],
    "activation_functions": ["relu","tanh","softmax"],
    "weight_range": {
      "from": -0.1,
      "to": 0.1
    },
    "bias_range": {
      "from": -0.1,
      "to": 0.1
    }
  },
  "training": {
    "epochs": 2000,
    "learning_rate": 0.001,
    "minibatch_size": 20,
    "loss_function": "CE",
    "optimizer": "ADAM",
    "test_frequency": 100
  },
  "visualization": {
    "on": false,
    "map_batch_size": 10,
    "display_weights": [],
    "display_biases": [],
    "display_layers": [],
    "dendrogram_layers": []
  }
}